pipeline
{
  agent any
  
    stages
    {
    
      stage("build")
      {   steps
            {
                echo 'build phase'
            }
      }
      stage("test")
      {   steps
            {
                echo 'test phase'
                echo 'second change in test '
            }
      }
      stage("deploy")
      {   steps
            {
                echo 'deploy phase'
            }
      }
      
    }
      
}
